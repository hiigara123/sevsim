%html
  %head
    %meta{ :"http-equiv" => "content-type", :content => "text/html; charset=utf-8" } 
    %meta{ :name => "viewport", :content => "width=device-width,initial-scale=1" }
    %script{ :src => "https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js" }

    %script{ :src => "https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore.js" }
    %script{ :src => "https://cdnjs.cloudflare.com/ajax/libs/underscore.string/3.0.3/underscore.string.js" }

    %script{ :src => "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js" }
      
    %script{ :src => "http://code.highcharts.com/highcharts.js" }
    %script{ :src => "js/tabletop.js" }
    %script{ :src => "js/stuff.js" }
    %script{ :src => "js/app.js" }
    %script{ :src => "js/chart.js" }
    %script{ :src => "js/action_manager.js" }
    %script{ :src => "js/actions.js" }
    %script{ :src => "js/app_view.js" }
    %script{ :src => "js/rainbowvis.js" }

    %link{ :rel => "stylesheet", :href => "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css" }
    %link{ :rel => "stylesheet", :href => "style.css" }
    %title Симулятор Севастопольца
  %body

    .container-fluid
      .row
        .col-xs-12
          #actions
            #actions-container

      .row
        .col-xs-9
          #chart-container
        .col-xs-3.scores-container
          .row.level
          .row
            %ul#scores
              %li#pain
                %b Унижение
                %span.val

              %li#moral
                %b Духовность
                %span.val

              %li#score
                %b Распилено бабла
                %span.val
          .row.face-container
            %img#face




